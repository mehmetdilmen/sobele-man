{
  "expo": {
    "name": "GameMobile",
    "slug": "GameMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.GameMobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.GameMobile"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "gamemobile",
    "experiments": {
      "tsconfigPaths": true
    },
    "navigationBar": {
      "visible": "never"
    },
    "plugins": ["expo-router"],
    "headers": {
      "headerShown": false
    }
  }
}
