{
  "expo": {
    "name": "Sobele Man",
    "slug": "sobele-man",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.GameMobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mehmetdilmen.sobeleman"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "gamemobile",
    "experiments": {
      "tsconfigPaths": true
    },
    "navigationBar": {
      "visible": "never"
    },
    "plugins": [
      "expo-router",
      [
        "expo-ads-admob",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you.",
          "appId": "ca-app-pub-4300902416268566~3882298439"
        }
      ]
    ],
    "headers": {
      "headerShown": false
    }
  }
}
